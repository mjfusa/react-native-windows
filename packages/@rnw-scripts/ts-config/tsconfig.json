{
  "compilerOptions": {
    "target": "ES2017",
    "module": "commonjs",
    "jsx": "react",
    "sourceMap": true,
    "declaration": true,
    "strict": true,
    "preserveConstEnums": true,
    "moduleResolution": "node",
    "noUnusedLocals": true,
    "noImplicitReturns": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
  }
}
