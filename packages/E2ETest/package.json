{
  "name": "e2etest",
  "version": "0.0.54",
  "private": true,
  "scripts": {
    "start": "react-native start",
    "lint": "rnw-scripts lint",
    "lint:fix": "rnw-scripts lint:fix",
    "watch": "rnw-scripts watch",
    "windows": "react-native run-windows",
    "e2e": "react-native run-windows --no-launch --logging && npm run e2etest",
    "e2ebundle": "npm run bundle && react-native run-windows --no-launch --no-packager --bundle --logging && npm run e2etest",
    "e2etest": "rimraf reports/* && wdio run ./wdio.conf.js --colors",
    "testspec": "wdio --spec",
    "bundle": "rnw-scripts prepareBundle && react-native bundle --platform windows --entry-file app/index.ts --bundle-output windows/ReactUWPTestApp/Bundle/index.windows.bundle --assets-dest windows/ReactUWPTestApp/Bundle",
    "buildapp": "react-native run-windows --no-launch --no-packager --no-deploy",
    "deployapp": "react-native run-windows --no-launch --no-packager --no-build",
    "buildbundleapp": "npm run bundle && react-native run-windows --no-launch --no-packager --no-deploy --bundle",
    "deploybundleapp": "react-native run-windows --no-launch --no-packager --no-build --bundle"
  },
  "dependencies": {
    "@react-native-windows/tester": "0.0.1",
    "react": "17.0.1",
    "react-native": "0.0.0-c66aa8abc",
    "react-native-windows": "^0.0.0-canary.245"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/runtime": "^7.8.4",
    "@rnw-scripts/eslint-config": "1.1.6",
    "@rnw-scripts/just-task": "2.0.1",
    "@rnw-scripts/ts-config": "1.1.0",
    "@types/jasmine": "^3.3.0",
    "@types/node": "^14.14.21",
    "@types/react": "16.9.11",
    "@types/react-native": "^0.63.46",
    "@wdio/cli": "^6.9.0",
    "@wdio/dot-reporter": "^6.8.1",
    "@wdio/jasmine-framework": "^6.11.0",
    "@wdio/junit-reporter": "^6.8.1",
    "@wdio/local-runner": "^6.12.1",
    "@wdio/spec-reporter": "^6.11.0",
    "eslint": "7.12.0",
    "just-scripts": "^1.3.2",
    "metro-react-native-babel-preset": "^0.56.0",
    "prettier": "1.19.1",
    "react-test-renderer": "17.0.1",
    "rimraf": "^3.0.0",
    "ts-node": "^7.0.1",
    "tsconfig-paths": "^3.8.0",
    "typescript": "^3.8.3",
    "wdio-winappdriver-service": "^0.1.0",
    "webdriverio": "^6.9.0"
  }
}
